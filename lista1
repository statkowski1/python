#zad1
a1 = 4
a2 = 8
b1 = 5.7
b2 = 7.2
c1 = 'min'
c2 = 'max'
print(a1)
print(a2)
print(b1)
print(b2)
print(c1)
print(c2)
-------------------------------------------------------------------------------------------------------------------------------
#zad2
print('1: + dodawanie')
print('2: - odejmowanie')
print('3: * mnozenie')
print('4: / dzielenie')
print('5: ** potegowanie')
print('6: % reszta z dzielenia')
print('7: // dzielenie bez reszty')
x = int(input('Podaj numer operacji ktora chcesz wykonac\n'))
a = float(input('Podaj pierwsza liczbe\n'))
b = float(input('Podaj druga liczbe\n'))
if x == 1:
    wynik = a + b
elif x == 2:
    wynik = a - b
elif x == 3:
    wynik = a * b
elif x == 4:
    wynik = a / b
elif x == 5:
    wynik = a ** b
elif x == 6:
    wynik = a % b
elif x == 7:
    wynik = a // b
else:
    print('Blad, wybierz operacje 1-7')
print(wynik)
-------------------------------------------------------------------------------------------------------------------------------
#zad3
a=int(12)
b=int(4)
print("Przykladowo: a=12, b=4 dla wszystkich dzialan")
print("a-=b")
a-=b
print(a)
a=12
b=4
print("a+=b")
a+=b
print(a)
a=12
b=4
print("a*=b")
a*=b
print(a)
a=12
b=4
print("a/=b")
a/=b
print(a)
a=12
b=4
print("a**b")
a**b
print(a)
a=12
b=4
print("a%=b")
a%=b
print(a)
